{
  "name": "js-es6-start",
  "version": "0.0.0",
  "description": "A javascript es6 starter pack for writing es6 throwaway js code easily",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir dist/index.js "
  },
  "bin": {
    "hello": "./bin/index.js"
  },
  "author": "Subash Poudel",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "npm-run-all": "^4.1.5"
  }
}
